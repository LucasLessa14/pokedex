CA;AACA;AAPA;AA7BA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n\n    <Navbar />\n    <div class=\"container\">\n      <div class=\"columns\">\n      <div class=\"column is-half is-offset-one-quarter desce\">\n        <img id=\"pokebola\" src=\"./assets/pokemon.svg\" alt=\"pokebola\">\n        <p class=\"is-size-3\">Pokedex</p>\n        <hr>\n        <input type=\"text\" placeholder=\"Buscar pokemon pelo nome\" class=\"input\" v-model=\"busca\" @keyup.enter=\"buscar\">\n        <!-- <button class=\"button is-fullwidth is-danger\" id=\"buscaBtn\" @click=\"buscar\">Buscar</button> -->\n\n        <div class=\"control has-icons-left\">\n        <div class=\"select\">\n          <select>\n            <option selected>Country</option>\n            <option>Select dropdown</option>\n            <option>With options</option>\n          </select>\n        </div>\n        <span class=\"icon is-left\">\n          <i class=\"fas fa-globe\"></i>\n        </span>\n      </div>\n      \n      </div>\n      </div>\n\n      <div id=\"poke-container\">\n        <div v-for=\"(poke,index) in filteredPokemons\" :key=\"poke.url\">\n          <Pokemon :name=\"poke.name\" :url=\"poke.url\" :num=\"index+1\"/>\n        </div>\n      </div>\n\n      <Pagination />\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport Pokemon from './components/Pokemon.vue';\nimport Pagination from './components/Pagination.vue'\nexport default {\n  name: 'App',\n  data(){\n    return {\n      pokemons: [],\n      filteredPokemons: [],\n      busca: ''\n    }\n  },\n  created: function() {\n    axios.get(\"https://pokeapi.co/api/v2/pokemon?limit=151&offset=0\").then(res => {\n      this.pokemons = res.data.results;\n      this.filteredPokemons = res.data.results;   \n    })\n  },\n  components: {\n    Pokemon,\n    Pagination\n  },\n  methods: {\n    buscar: function() {\n      this.filteredPokemons = this.pokemons;\n      if(this.busca == '' || this.busca == ' '){\n        this.filteredPokemons = this.pokemons;\n      } else {\n        this.filteredPokemons = this.pokemons.filter(pokemon => pokemon.name == this.busca);\n      }\n    }\n  },\n  computed: {\n    // resultadoBusca: function() {\n    //   if(this.busca == '' || this.busca == ' '){\n    //     return this.pokemons;\n    //   } else {\n    //     return this.pokemons.filter(pokemon => pokemon.name == this.busca);\n    //   }\n    // }\n  }\n}\n</script>\n\n<style>\n  #app {\n    font-family: Avenir, Helvetica, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osz-font-smoothing: grayscale;\n    text-align: center;\n    color: #223355;\n  }\n\n  .desce {\n    margin-top: 70px;\n  }\n\n  button {\n    font-weight: bold;\n  }\n\n  #buscaBtn {\n    margin-top: 2%;\n  }\n\n  #pokebola {\n    max-width: 50px;\n  }\n\n  #poke-container {\n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    gap: 10px;\n  }\n</style>\n"],"sourceRoot":"src"}]}